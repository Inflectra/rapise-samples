    <TestFiles>
      <TestFile>
        <Path>Main.js</Path>
        <Tags />
      </TestFile>
      <TestFile>
        <Path>Main.rvl.xlsx</Path>
        <Tags />
      </TestFile>
      <TestFile>
        <Path>Objects.js</Path>
        <Tags />
      </TestFile>
      <TestFile>
        <Path>..\User.js</Path>
        <Tags />
      </TestFile>
    </TestFiles>
  </TestFiles>
